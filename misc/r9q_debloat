err_red() {
    printf "%b[%s] =>>: %b%s%b\n" '\033[1;31m' "$(date "+%H:%M:%S")" \
                                  '\033[1;37m' "$*" '\033[0m' 1>&2
}

info_blue() {
    printf "%b[%s] =>>: %b%s%b\n" '\033[1;34m' "$(date "+%H:%M:%S")" \
                                  '\033[1;37m' "$*" '\033[0m'
}

info_green() {
    printf "%b[%s] =>>: %b%s%b\n" '\033[1;32m' "$(date "+%H:%M:%S")" \
                                  '\033[1;37m' "$*" '\033[0m'
}

info_white() {
    printf "%b[%s] =>>: %b%s%b\n" '\033[1;37m' "$(date "+%H:%M:%S")" \
                                  '\033[1;37m' "$*" '\033[0m'
}

pkg_rm() (
    _errh() { return "$1"; }
    trap '_errh $?' INT TERM EXIT; set -e

    DIR='/storage/emulated/0/Android'

    pm clear --user 0 "$1" > /dev/null 2>&1 || :
    { OP=$(pm uninstall --user 0 "$1") && RC=0; } || RC="$?"
    if [ "$RC" -eq 0 ]; then
        info_blue "pkg_rm: $1:"; info_green "$OP"
    else
        { info_blue "pkg_rm: $1:"; err_red "$OP"; } >&2
        return "$RC"
    fi
    rm -rf "$DIR/data/$1" "$DIR/media/$1" "$DIR/obb/$1" 2> /dev/null
)

pkg_clr() (
    _errh() { return "$1"; }
    trap '_errh $?' INT TERM EXIT; set -e

    DIR='/storage/emulated/0/Android'

    { OP=$(pm clear --user 0 "$1") && RC=0; } || RC="$?"
    if [ "$RC" -eq 0 ]; then
        info_blue "pkg_clr: $1:"; info_green "$OP"
    else
        { info_blue "pkg_clr: $1:"; err_red "$OP"; } >&2
        return "$RC"
    fi
    rm -rf "$DIR/data/$1"/* 2> /dev/null
)

